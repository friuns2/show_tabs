{
  "name": "Show Tabs in Process",
  "version": "1.0",
  "description": "Adds a browser action showing which tabs share the current tab's process.",
    "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "contextMenus"
  ],
  "background": {
    "scripts": ["events.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "Show Tabs in this Process",
      "default_icon": "icon.png"
      
  },
  "manifest_version": 2
}

